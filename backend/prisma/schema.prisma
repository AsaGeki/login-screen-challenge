datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model users {
    id        Int      @id @default(autoincrement())
    username  String   @unique
    email     String   @unique
    password  String
    createdAt DateTime @default(now())
}

model temporary_user {
    username String @unique
    email    String @unique
    password String
}
