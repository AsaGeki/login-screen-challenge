datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id       Int      @id @default(autoincrement())
    username String   @unique
    gmail    String   @unique
    password String
    status   Boolean  @default(false)
    updateAt DateTime @default(now()) @updatedAt
}
